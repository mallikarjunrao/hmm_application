 .img-txt {
    font-family:"Times New Roman", Times, serif;
    font-size:12px;
    color: #49392a;
    text-decoration:none;
    font-weight:bold;
  }

  .backround-img {
    background-image:url(/images/cute_kids_contest/backround-img.jpg);
    background-position:center top;
    background-repeat:no-repeat;
    height:699px;
  }

  .backroung-img-bg {
    background-image:url(/images/cute_kids_contest/backround-img-bg.jpg);
    background-repeat:repeat-y;
    background-position:center top;
  }

  .cont-big {
    font-family:"Futura Bk BT";
    font-size:20px;
    color:#49392a;
    line-height:26px;
    text-decoration:none;
    font-weight:normal;
  }

  .cont-small {
    font-family:"Futura Bk BT";
    font-size:15px;
    color:#695139;
    line-height:20px;
    text-decoration:none;
    font-weight:normal;
  }