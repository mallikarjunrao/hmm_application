.newBtnCont{
    font-size:17px;
    color:#fff;
    text-decoration:none;
    width:250px;
    padding:15px 0;
    margin-bottom:20px;
    text-align:center;
    behavior: url(PIE.htc);
    border-radius: 10px;
    border:1px solid #426983;
    -moz-border-radius: 10px; -ms-border-radius: 10px; -o-border-radius: 10px; -khtml-border-radius: 10px;
    background: #194b6b;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#0e344d),(#194b6b), to(#0e344d));
    background-image: -webkit-linear-gradient(top, #0e344d,#194b6b,#0e344d);
    background-image: -moz-linear-gradient(top, #0e344d,#194b6b,#0e344d);
    background-image: -ms-linear-gradient(top, #0e344d,#194b6b,#0e344d);
    background-image: -o-linear-gradient(top, #0e344d,#194b6b,#0e344d);
    background-image: linear-gradient(top, #0e344d,#194b6b,#0e344d);
    -pie-background: linear-gradient(#0e344d,#194b6b,#0e344d); /*PIE*/
    display:block;
}
.newBtnCont:hover{ background: #002943;  background-image: -webkit-gradient(linear, left top, left bottom, from(#002943),(#194b6b), to(#002943));
                   background-image: -webkit-linear-gradient(top, #002943,#194b6b,#002943);
                   background-image: -moz-linear-gradient(top, #002943,#194b6b,#002943);
                   background-image: -ms-linear-gradient(top, #002943,#194b6b,#002943);
                   background-image: -o-linear-gradient(top, #002943,#194b6b,#002943);
                   background-image: linear-gradient(top, #002943,#194b6b,#002943);
                   -pie-background: linear-gradient(#002943,#194b6b,#002943); /*PIE*/
}
