            <tr>
              <th align="left" valign="top" scope="col"></th>
              <th height="42" align="center" valign="top" class="text1" scope="col">You must first read the following:</th>
              <th align="left" valign="top" scope="col"></th>
            </tr>
            <tr>
              <th align="left" valign="top" scope="col"></th>
              <th align="left" valign="top" class="text1" scope="col">HoldMyMemories.com contests Terms and Conditions:</th>
              <th align="left" valign="top" scope="col"></th>
            </tr>
            <tr>
              <th align="left" valign="top" scope="col"></th>
              <th height="26" align="left" valign="top" class="cont" scope="col">&nbsp;</th>
