package events
{
	import flash.events.Event;
	
	public class SlideShowEvent extends Event
	{
		public static const CHECKBOXCHECKED : String = "checkboxchacked";
		public static const CHECKBOXUNCHECKED : String = "checkboxunchecked";
		public var data : Object;
		
		public function SlideShowEvent(type : String, data : Object)
		{
			super(type);
			this.data = data;
		}

	}
}