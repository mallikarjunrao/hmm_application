<table border="1" cellpadding="0" cellspacing="0" align="center" width="900" style="margin-top: 30px">
  <tr><td colspan="4" align="center"><h3>Excel Report for Zipcodes</h3></td></tr>
  <tr>
    <th>Zip Code</th>
    <th>Miles</th>
    <th>IP Address</th>
    <th>Added Date</th>
  </tr>
   <%for zip in @zips%>
    <tr>
      <td align="center"><%=zip.zip_code%></td>
      <td align="center"><%=zip.miles%></td>
      <td align="center"><%=zip.ip_address%></td>
      <td align="center"><%=zip.created_at%></td>

    </tr>
  <%end%>
</table>