
xml.instruct! :xml, :version=>"1.0" 
xml.contents{
  for content in @contents
  xml.content{
      xml.flv(content.img_url+"/user_content/audios/"+content.v_filename)
      xml.icon(content.img_url+"/user_content/audios/speaker.jpg")
      xml.name(content.v_tagphoto)
      xml.id(content.id)
      xml.hmmtype(content.e_filetype)
      }
  end
      

}