<script language="JavaScript">
  var jsReady = true;
  function blockNavigation() {
    if (document.all) // for IE
    {
      document.onclick=noClick;
    }
    else // for other browsers
    {
      document.onclick=clickMozilla;
    }
    return jsReady;
  }

  function unBlockNavigation() {
    if (document.all) // for IE
    {
      document.onclick = clickEnable;
    }
    else
    {
      document.onclick = clickEnableMozilla;
    }
    return jsReady;
  }

  function noClick() {
    if (event.button==0) {
      var where_to= confirm("Upload is in progress,\n Do you want to continue?");
      if (where_to == true) {
        return true;
      }
      else
      {
        return false;
      }
    }
  }
  function clickMozilla(event) {

    if ((event.button==0)||(event.button==2)) {
      var where_to= confirm("Upload is in progress,\n Do you want to continue?");
      if (where_to == true) {
        return true;
      }
      else
      {
        return false;
      }

    }
  }
