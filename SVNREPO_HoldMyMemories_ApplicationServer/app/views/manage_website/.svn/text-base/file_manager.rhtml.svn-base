    if ((event.button==1)||(event.button==2)) {
    }
  }
  function clickEnableMozilla(event)
  {
    if ((event.button==0)||(event.button==2)) {

      return true;
    }
  }

  function page_reload()
  {location.reload(true);}

</script>
