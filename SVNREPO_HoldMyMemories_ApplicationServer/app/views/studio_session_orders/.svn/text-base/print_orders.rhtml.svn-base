<%= javascript_include_tag "themes/nyromodal/jquery-1.3.2.min" %>
<%= javascript_include_tag "themes/nyromodal/jquery.nyroModal-1.5.2.pack.js" %>
<%= stylesheet_link_tag '/stylesheets/themes/nyromodal/nyroModal'%>
<%= javascript_include_tag "themes/swfobject" %>

<table width="970" border="0" cellspacing="0" cellpadding="0">
   <tr>
      <td height="1200" align="center" valign="top">
        <div id="flash_content">
          <h1>Get Flash Player</h1>
          <p><a href="http://www.adobe.com/go/getflashplayer"><img src="http://www.adobe.com/images/shared/download_buttons/get_flash_player.gif" alt="Get Adobe Flash player" /></a></p>
        </div>
        <script type="text/javascript">
          var flashvars = {};
          flashvars.proxyurl = "<%= @path1.content_path %>,<%=  @content_server_url %>";
          flashvars.familyname = "<%=  params[:id] %>";
          flashvars.user_id = "<%= @family_website_owner.id %>"
          flashvars.visitor_id = "<%= session[:visitor_id] %>"
          flashvars.website_type = "familywebsite_v3";
          flashvars.cart_url = "/carts/cart_list/<%=@family_website_owner.family_name %>"
          var params = {};
          params.wmode = "transparent";
         swfobject.embedSWF("/flash/debug/NewPrintOrder.swf", "flash_content", "950", "1200", "9.0.0", "/expressInstall.swf",flashvars,params);
        </script>
      </td>
    </tr>
</table>