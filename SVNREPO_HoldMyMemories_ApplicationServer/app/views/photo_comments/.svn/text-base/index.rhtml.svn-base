<h1>Listing photo_comments</h1>

<table>
  <tr>
    <th>V title</th>
  </tr>
  
<% for photo_comment in @photo_comments %>
  <tr>
    <td><%=h photo_comment.v_title %></td>
    <td><%= link_to 'Show', photo_comment_path(photo_comment) %></td>
    <td><%= link_to 'Edit', edit_photo_comment_path(photo_comment) %></td>
    <td><%= link_to 'Destroy', photo_comment_path(photo_comment), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New photo_comment', new_photo_comment_path %>