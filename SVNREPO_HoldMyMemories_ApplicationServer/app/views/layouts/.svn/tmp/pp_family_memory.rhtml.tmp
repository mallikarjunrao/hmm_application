    <%
    @share_image = "#{@family_website_owner.img_url}/hmmuser/familyphotos/thumbs/#{@family_website_owner.family_pic}" if @share_image.blank?
    @share_type = 'image' if @share_type.blank?
    @share_video = '' if @share_video.blank?
    if(@title.nil? || @title.blank?)
      if(@detail)
        if(@store=="" || @store==nil)
          @title="Brought to you by HoldMyMemories.com"
        else
          @title="Brought to you by #{@store} Studios"
        end
      else
        @title="Brought to you by HoldMyMemories.com"
      end
    else
      @title=@title
    end

    @studio_header_img="default_top_logo_white.png" #default logo depending upon the header logo color specified in the themes table
    @studio_website = "/" #default studio link

    unless @family_website_owner.studio_id==0
      @studio_header = HmmStudio.find(@family_website_owner.studio_id)
      if  @studio_header.booking_link.nil? ||  @studio_header.booking_link.blank?
        link="/my_familywebsite/book_session/?studio_id=#{@studio_header.id}"
      else
        link="#{@studio_header.booking_link}"
      end
      if @studio_header
        @studio_header_img_url = @studio_header.img_url
        @studio_header_img = "#{@studio_header.studio_top_logo}_white.png" if @studio_header.studio_top_logo != nil
        @studio_website = @studio_header.studio_website if @studio_header.studio_website != nil
      end
    end
  %>

    <script type="text/javascript">
      fullurl=document.URL;
      url=fullurl.split("/");
      for(i=0;i<=url.length;i++)
      {
        if(url[i])
        {
          url[i]=url[i].toUpperCase();
          if((url[i].indexOf("%3CSCRIPT%3E") >= 0) || (url[i].indexOf("%3C/SCRIPT%3E") >= 0) || (url[i].indexOf("%3C/SCRIP%3E") >= 0) || (url[i].indexOf("%3CSCRIP") >= 0) ||(url[i].indexOf("JAVASCRIPT:ALERT") >= 0))
          {
            window.location="/contests/error_page"
          }
        }
      }
    </script>

    <script type="text/javascript">
      function facebook_share_image(url)
      {

        document.getElementById('share_image').value=url;

      }

      function fbs_click_facebook(id) {

        new Ajax.Request("/contests/create_sharecount/"+id+"?from=facebook");

        return false;
      }
      function fbs_click_myspace(id)
      {

        new Ajax.Request("/contests/create_sharecount/"+id+"?from=myspace");
        return false;
      }

      var Up = jQuery.noConflict();
      Up(document).ready(function(){
        function preloadImg(image) {
          var img = new Image();
          img.src = image;
        }
        preloadImg('/images/ajaxLoader.gif');

        Up.nyroModalSettings({
          minWidth: 350,
          minHeight: 410,
          autoSizable: true,
          css: {
            content: {
              background: '#9FC4DE',
              overflow: 'auto'
            },
            wrapper: {
              background: '#9FC4DE',
              width: '344px'
            },
            contentWrapper:{
              background: '#9FC4DE',
              width: '344px'
            }
          }
        });
      });

    </script>
