    }
    if (document.frm.licensor_address.value == '')
    {
      alert("Please enter the Licensor Address");
      document.frm.licensor_address.focus();
      return false;
    }
    if (document.frm.licensor_attn.value == '')
    {
      alert("Please enter the Licensor Attn");
      document.frm.licensor_attn.focus();
      return false;
    }
    if (document.frm.licensor_fax.value == '')
    {
      alert("Please enter the Licensor Fax");
      document.frm.licensor_fax.focus();
      return false;
    }
    if (document.frm.licensor_studio.value == '')
    {
      alert("Please enter the Licensor Studio");
      document.frm.licensor_studio.focus();
      return false;
    }
    if (document.frm.licensor_studio_by.value == '')
    {
      alert("Please enter the Licensor Name");
      document.frm.licensor_studio_by.focus();
      return false;
    }
    if (document.frm.licensor_studio_title.value == '')
    {
      alert("Please enter the Licensor Title");
      document.frm.licensor_studio_title.focus();
      return false;
    }
	
