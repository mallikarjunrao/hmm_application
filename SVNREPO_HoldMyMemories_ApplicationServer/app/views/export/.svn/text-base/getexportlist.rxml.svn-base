xml.instruct! :xml, :version=>"1.0"
xml.exports{
  if(@exportlist != nil)
    for @exportdetails in @exportlist
      xml.export{
        xml.name(@exportdetails.v_user_name)
        xml.status(@exportdetails.status)
        xml.type(@exportdetails.export_type)
        xml.image(@exportdetails.img_url+"/hmmuser/photos/thumb/"+@exportdetails.v_myimage)
      }
    end
  end
}