<h1>Listing book_sessions</h1>

<table>
  <tr>
  <% for column in BookSession.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for book_session in @book_sessions %>
  <tr>
  <% for column in BookSession.content_columns %>
    <td><%=h book_session.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => book_session %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => book_session %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => book_session }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>
<%=will_paginate @book_sessions%>
<br />

<%= link_to 'New book_session', :action => 'new' %>
