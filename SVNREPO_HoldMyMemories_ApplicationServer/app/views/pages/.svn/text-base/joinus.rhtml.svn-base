<script language="JavaScript">
  function chk_verify()
{
	if(document.form1.sname.value=="")
	{
		alert("Please enter Studio Name")
		document.form1.sname.focus()
		return false;
  	}

	if(document.form1.sphone.value=="")
	{
		alert("Please enter Studio Phone Number")
		document.form1.sphone.focus()
		return false;
  	}
        else
        {
            if(!document.form1.sphone.value.match(/^[0-9]|[-]$/))
           {
              alert("Enter numeric value for Studio Phone Number")
              document.form1.sphone.focus()
              return false;
           }
           

        }

	if(document.form1.name.value=="")
	{
		alert("Please enter Name")
		document.form1.name.focus()
		return false;
  	}
	if(document.form1.phone.value=="")
	{
		alert("Please enter Phone Number")
		document.form1.phone.focus()
		return false;
  	}
        else
        {
            if(isNaN(document.form1.phone.value))
           {
              alert("Enter numeric value for Phone Number")
              document.form1.phone.focus()
              return false;
           }
        }
	if(document.form1.email.value=="")
	{
		alert("Please enter Email Address")
		document.form1.email.focus()
		return false;
 	}
	var email = document.form1.email.value
	var regexpr=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i  //email check
	if (!regexpr.test(email))
	{
		alert("Please input a valid Email Address")
		document.form1.email.value=""
		document.form1.email.focus();
		return false;
	}



	return true;






}


</script>



