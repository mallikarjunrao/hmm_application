<h1>Listing nonhmm_users</h1>

<table>
  <tr>
  <% for column in NonhmmUser.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for nonhmm_user in @nonhmm_users %>
  <tr>
  <% for column in NonhmmUser.content_columns %>
    <td><%=h nonhmm_user.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => nonhmm_user %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => nonhmm_user %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => nonhmm_user }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @nonhmm_user_pages.current.previous } if @nonhmm_user_pages.current.previous %>
<%= link_to 'Next page', { :page => @nonhmm_user_pages.current.next } if @nonhmm_user_pages.current.next %> 

<br />

<%= link_to 'New nonhmm_user', :action => 'new' %>
