                function runSlideShow(item){
                    if (document.all){
                        document.images.photoslider.style.filter="blendTrans(duration=2)";
                        document.images.photoslider.style.filter="blendTrans(duration=CrossFadeDuration)";
                        document.images.photoslider.filters.blendTrans.Apply();}
                    if(item==0)
                        document.images.photoslider.src = preLoad[jss].src;

                    current_img = document.images.photoslider.src;
                    temp_img = current_img.split('/');
                    image_name = temp_img[temp_img.length-1]
                   // alert(image_name);
                    if (image_name=='memoriesmission.jpg' || image_name=='memoriesmission.jpg' ||  item=='1')
