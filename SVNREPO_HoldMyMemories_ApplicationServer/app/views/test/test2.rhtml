                        document.getElementById('img1').onmouseout= MM_swapImgRestore;
                        //document.getElementById('Image11').setAttribute("onmouseout", "MM_swapImgRestore();");
                    }
                    if (image_name=='memorieslifetime.jpg' || image_name=='memorieslifetime.jpg' || item=='2')
                    {
                        document.getElementById('imgurl').href="/static/studios"
	
                        document.getElementById('Image12').src='/new_images/memories_lifetime_ovr.jpg';
                        document.getElementById('img2').onmouseout="";
	
                    }else{
