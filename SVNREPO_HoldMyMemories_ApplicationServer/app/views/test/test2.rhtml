                        document.getElementById('img2').onmouseout= MM_swapImgRestore;

                    }
                    if (image_name=='holdorganize.jpg' || image_name=='holdorganize.jpg' || item=='3')
                    {
                        document.getElementById('imgurl').href="/bob"	
                        document.getElementById('Image13').src='/new_images/hold_organize_ovr.jpg';
                        document.getElementById('img3').onmouseout="";
                    }else{
                        document.getElementById('Image13').src='/new_images/hold_organize_def.jpg';
                        document.getElementById('img3').onmouseout= MM_swapImgRestore;
                    }
                    if (image_name=='timeshine.jpg' || image_name=='timeshine.jpg' ||  item=='4')
                    {
                        document.getElementById('imgurl').href="/contests/hmm_contest"	
                        document.getElementById('Image14').src='/new_images/time_shine_ovr.jpg';
                        document.getElementById('img4').onmouseout='';
                    }else{
                        document.getElementById('Image14').src='/new_images/time_shine_def.jpg';
                        document.getElementById('img4').onmouseout= MM_swapImgRestore;
                    }

                    if (document.getElementById) document.getElementById("image");


                    if (document.all) document.images.photoslider.filters.blendTrans.Play();
                    jss = jss + 1;
                    if (jss > (pss)) jss=1;
                    tss = setTimeout('runSlideShow(0)', SlideShowSpeed);
                    //alert(document.getElementById('Image12').src);
                }

                function showPopup() {
                    var popup = window.open(this.href, "popup", "height=800,width=600");
