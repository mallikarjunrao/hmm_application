<h1>Listing my_carts</h1>

<table>
  <tr>
  <% for column in MyCart.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for my_cart in @my_carts %>
  <tr>
  <% for column in MyCart.content_columns %>
    <td><%=h my_cart.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => my_cart %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => my_cart %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => my_cart }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @my_cart_pages.current.previous } if @my_cart_pages.current.previous %>
<%= link_to 'Next page', { :page => @my_cart_pages.current.next } if @my_cart_pages.current.next %> 

<br />

<%= link_to 'New my_cart', :action => 'new' %>
