<h1>Listing business_managers</h1>

<table>
  <tr>
  <% for column in BusinessManager.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for business_manager in @business_managers %>
  <tr>
  <% for column in BusinessManager.content_columns %>
    <td><%=h business_manager.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => business_manager %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => business_manager %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => business_manager }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @business_manager_pages.current.previous } if @business_manager_pages.current.previous %>
<%= link_to 'Next page', { :page => @business_manager_pages.current.next } if @business_manager_pages.current.next %> 

<br />

<%= link_to 'New business_manager', :action => 'new' %>
