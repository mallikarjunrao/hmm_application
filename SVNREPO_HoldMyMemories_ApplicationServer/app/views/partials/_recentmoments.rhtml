<table width="954" border="0" cellspacing="0" cellpadding="0" id="tab_bgcolor">
  <tr>
    <td width="25" align="left" valign="middle" background="/images/common/tab_trans.png" ></td>
    <td width="929" height="36" align="left" valign="middle" background="/images/common/tab_trans.png" class="headers">Recently Added Moments</td>
  </tr>
  <tr>

    <td height="155" align="center" valign="middle" colspan="2">

      <table width="930" height="93" cellpadding="0" cellspacing="0">
        <tr valign="middle">
         <% if  @recent_moments %>
          <% for recent_moment in @recent_moments %>
            <td align="center">
              <a href="/familywebsite/moments/<%=params[:id]%>?moment_id=<%=  recent_moment.id %>">
                <img src="<%= recent_moment.img_url %>/user_content/photos/big_thumb/<%=  recent_moment.v_filename %>"  height="86"  alt="" border="0" />
              </a>
            </td>
          <% end %>
            <% end %>
        </tr>
      </table>
    </td>
  </tr>
</table>