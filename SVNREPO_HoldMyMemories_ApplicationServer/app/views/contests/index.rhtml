        <tr>
          <td height="23" align="center" valign="middle" colspan="2">&nbsp;</td>
          <td align="left" valign="top" colspan="2">&nbsp;</td>
        </tr>
        <tr>
          <% a = ContestDetail.first(:conditions => "name = 'thatsMYbaby'") %>
          <td width="420" height="23" align="center" valign="middle" colspan="2">
            <a href="/contests/<%= a.id %>/hmm_contest"><img src="<%= a.photo_logo %>" border="0"/></a>
          </td>
          <% b = ContestDetail.first(:conditions => "name = 'Precious Pets'") %>
          <td width="420" align="center" valign="middle" colspan="2">
            <a href="/contests/<%= b.id %>/hmm_contest"><img src="<%= b.photo_logo %>" width="349" height="187"  border="0" /></a>
          </td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td align="left" valign="top"><img src="/images/all_american_family/contest_select_pages/bot-curve.png" width="840" height="21" /></td>
  </tr>
