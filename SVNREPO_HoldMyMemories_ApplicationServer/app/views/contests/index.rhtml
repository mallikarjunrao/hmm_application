  <tr>
    <td align="center" valign="top"><a href="/contests/winners/previous_contest"><img src="/images/all_american_family/contest_select_pages/previous-contest-winners.png" width="298" height="48" border="0"/></a></td>
  </tr>
  <tr>
    <td align="left" valign="top">&nbsp;</td>
  </tr>
  <tr>
    <td align="left" valign="top">&nbsp;</td>
  </tr>
</table>


