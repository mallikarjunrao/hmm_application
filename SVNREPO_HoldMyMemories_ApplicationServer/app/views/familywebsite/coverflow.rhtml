<html>
  <head>
    <title>Coverflow Test page</title>
  </head>
  <body>

    <object width='954' height='500'>
      <param name='movie' value='"/Coverflow.swf'/>
      <param name='wmode' value='transparent'/>
      <param name='allowScriptAccess' value='sameDomain'/>
      <param name='FlashVars' value='proxyurl=<%= @content_server_url %>'/>
      <param name="allowFullScreen" value="true" />
      <embed src='/Coverflow.swf' allowFullScreen="true" width='954' height='500' wmode='transparent' FlashVars='proxyurl=<%= @content_server_url %>'>
      </embed>
    </object>
  </body>
</html>

